<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts PUBLIC
          "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"       
          "http://struts.apache.org/dtds/struts-2.0.dtd">
<!-- - This file is included by the struts.xml file as an example - of how 
	to break up the configuration file into multiple files. -->
<struts>
	<package name="welcome" extends="struts-default" namespace="/">
		<action name="welcome" method="execute" class="com.zhenghu.demo.web.actions.WelcomeAction">
			<result name="success">jsp/Information.jsp</result>
			<result name="input">jsp/Welcome.jsp</result>
		</action>
	</package>
</struts>
